alias gs='git status'
alias gl1='git log -1'
alias glog='git log --oneline --graph'
alias gcam='git commit -a -m'
alias gcaac='git commit -a --amend -CHEAD'
alias ginitial='git commit --allow-empty -m "Initial empty commit"'

alias be='bundle exec'
alias recreate_test_db='bundle exec rails db:environment:set RAILS_ENV=test && bundle exec rails db:drop db:create db:migrate RAILS_ENV=test'
alias se='stack exec'
alias sb='stack build'
alias yrp='yarn run pulp build'

